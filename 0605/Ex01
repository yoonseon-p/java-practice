package basic;
/*
 * 
 * method == function
 * 메소드		 함수
 * function 비 객체지향 언어에서
 * 
 */
public class Ex01 {
	
	public static void method1() {
		System.out.println("method1은 메소드 또는 함수인데 입력값이 없고 출력값이 없는 형태");
	}
	
	public static void method2(int value) {
		System.out.println("method2는 메소드 또는 함수인데 입력값이 있고 출력값이 없는 형태");
	}
	
	public static int method3(int value) {
		System.out.println("method3은 메소드 또는 함수인데 입력값이 있고 출력값도 있는 형태");
		return value;
	}
	
	public static int method4() {
		System.out.println("method4는 메소드 또는 함수인데 입력값이 없고 출력값이 있는 형태");
		return 0;
	}
	
	public static void main(String[] args) {
		// 변수를 호출할때는 변수 이름만 사용
		// 메소드를 호출때는 이름()
		System.out.println("함수 실행 전");
		method3(10);
		System.out.println("함수 실행 후");
	}

}
